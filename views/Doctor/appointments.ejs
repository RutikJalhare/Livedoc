<script>
   
    //  performing  status update functionality 
    function statusupdate (id, status   )  {
         console.log("status update" , id , status     )

         $.ajax({
            url: "http://localhost:5000/Doctor/statusupdate",
            method: "post",
            dataType: "JSON",
            data: {id:id , status:status},
            success: function (data) {
              console.log("data" , data) 
              if (data.msg == 200) {
          Swal.fire({
            title: "Appointment"+ "  "+status,
            icon: "success"
          });

        }


            }
        });




        load_data();









    }



    $(document).ready(function () {
        load_data();
    });
    function load_data() {
        $.ajax({
            url: "http://localhost:5000/Doctor/getallappointments",
            method: "get",
            dataType: "JSON",
            success: function (data) {
              
                var html = '';
                console.log(data)
                for (var count = 0; count < data.length; count++) {

                    let href =  "patient_profile/" + data[count].P_id +"/" +data[count].id
 console.log("date" , data[count])
 console.log( "patient_profile/" + data[count].P_id  )

                html +=`

            <div class="col-md-6 col-sm-6  col-lg-6">
                        <div class="profile-widget">
                            <div class="doctor-img">
                                <a class="avatar" href= `+ href +`   ><img alt="" src=`+ data[count].photo +`   >  </a>
                            </div>
                            <div class="dropdown profile-action">
                                <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown"
                                    aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                <div class="dropdown-menu dropdown-menu-right">
                                    <button class="dropdown-item"     onclick= "statusupdate(`  + data[count].id + ` , 'confirmed' ) "> <i class="fa fa-pencil m-r-5"></i>
                                        confirmed</button>

                                </div>
                            </div>
                            <h4 class="doctor-name text-ellipsis"><a href="patient_profile">`+ data[count].f_name +" " +data[count].l_name +`</a></h4>
                            <div class="doc-prof"> `+ data[count].email +`   </div>
                            <div class="doc-prof">+91 `+ data[count].contact +`</div>
                            <div class="user-country">
                                <i class="fa fa-map-marker"></i> `+ data[count].address +`
                            </div>

                            <p class="user-country">
                                Appointment Date : `+ data[count].date +` <br /> Slot : `+ data[count].slot +`
                            </p>
                            <p class="user-country text text-danger">
                                Appointment Type : `+ data[count].appointment_type +` <br />
                            </p>
                            <h4 class="doctor-name text-ellipsis text-dark lead"> Status : `+ data[count].status +`</h4>

                            <div class="user-country">
                                <div class="m-t-20 text-center">
                                    <button class="btn btn-danger submit-btn"    onclick= "statusupdate(`  + data[count].id + ` , 'cancel' ) "> Cancel</button>
                                </div>
                            </div>


                        </div>
            </div>
        </div>    
                
                    `
        }
                $('#appointment').html(html);

                 

            }
        });
    }


</script>